{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "build/**"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"lint": {},
		"format": {},
		"test": {
			"dependsOn": ["^build"]
		},
		"test:watch": {
			"cache": false,
			"persistent": true
		}
	}
}
